@inject ERPv1.Data.ApplicationDbContext _dbcontex
@model dynamic
@{
    var Custody = _dbcontex.AccountChart.Where( x=>!x.IsParent && x.AccTypeId ==9).Select(x => new SelectListItem
    {
        Value = x.AccNum.ToString(),
        Text = x.AccountNameAr
    }
    ).OrderBy(x => x.Text).ToList();

}
@{
    var htmlAtt = ViewData["htmlAtt"] ?? new { };

}
@Html.DropDownList("", Custody, "--اختار--", htmlAtt)


