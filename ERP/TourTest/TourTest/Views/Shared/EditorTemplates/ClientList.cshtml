@inject TourTest.Data.ApplicationDbContext _dbcontex
@model dynamic
@{ 
    var client = _dbcontex.Clients.Select(x => new SelectListItem
    {
        Value = x.Id.ToString(),
        Text = x.NameClient
    }
    ).OrderBy(x => x.Text).ToList();
}
@{ 
    var htmlAttributes = ViewData["htmlAttributes"] ?? new { };
}
@Html.DropDownList("", client, "--اختار--", htmlAttributes)